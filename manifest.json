{
    "name": "UoE INSIS Grade Grapher",
    "version" : "2.0",
    "manifest_version" : 2,
    "permissions": [ "tabs" ],
    "content_scripts" : [
        {
            "matches": ["*://insis.vse.cz/*"],
            "js": ["content.js"]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_title": "Grade Grapher"
    },
    "background": {
        "scripts": ["background.js"],
        "persistent": false
    },
    "icons": { "256": "/icon/icon256.png"
    }
    
}